{
  "name": "@asteres/neatui-vue",
  "description": "基于 Vue3 的 UI 组件库",
  "version": "0.19.1",
  "type": "module",
  "scripts": {
    "g:c": "node scripts/index.js template",
    "lib:build": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "inspect:eslint": "eslint --inspect-config",
    "push": "pwsh scripts/publish.ps1 push",
    "pull": "pwsh scripts/publish.ps1 publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@eslint/markdown": "^6.1.0",
    "@types/node": "^22.7.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "eslint": "^9.11.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "less": "^4.2.0",
    "prettier": "^3.3.3",
    "shiki": "^1.18.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.7",
    "vite-plugin-dts": "4.2.2",
    "vitepress": "^1.3.4",
    "vue-tsc": "^2.1.6"
  },
  "peerDependencies": {
    "vue": "^3.3.11"
  },
  "files": [
    "lib",
    "style"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "./style/*": "./style/*",
    "./lib/*": "./lib/*",
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DvShu/neatui-vue.git"
  },
  "keywords": [
    "neatui-vue",
    "neatui",
    "vue",
    "ui framework",
    "component library"
  ],
  "homepage": "https://dvshu.github.io/neatui-vue/",
  "bugs": {
    "url": "https://github.com/DvShu/neatui-vue/issues"
  },
  "license": "MulanPSL-2.0",
  "dependencies": {
    "@tanstack/vue-table": "^8.20.5",
    "ph-utils": "^0.9.3",
    "qrcode-generator-es": "^1.0.3",
    "vue-router": "^4.4.5"
  },
  "packageManager": "pnpm@9.0.2"
}
