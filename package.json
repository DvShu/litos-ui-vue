  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "g:c": "bun scripts/index.js template",
    "build": "bun scripts/index.js build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "inspect:eslint": "eslint --inspect-config"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.24.0",
    "globals": "^15.0.0",
    "less": "^4.2.0",
    "ph-utils": "^0.4.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "^1.0.2",
    "vue-tsc": "^2.0.6"
  },
  "peerDependencies": {
    "ph-utils": "^0.4.2",
    "vue": "^3.3.11"
  },
  "files": [
    "lib",
    "style"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
